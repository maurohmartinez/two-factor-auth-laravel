{
  "name": "mhmartinez/two-factor-authentication-laravel",
  "description": "A package to handle 2FA authentication",
  "type": "library",
  "require": {
    "php": "^8.0",
    "pragmarx/google2fa-qrcode": "^3.0",
    "pragmarx/google2fa-laravel": "^2.0",
    "laravel/framework": "^8.0.0|^9.0.0",
    "bacon/bacon-qr-code": "^2.0.0"
  },
  "keywords": [
    "laravel",
    "backpack",
    "two factor authentication",
    "google authentication for laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "maurohmartinez",
      "email": "maurohmartinez@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "MHMartinez\\TwoFactorAuth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MHMartinez\\TwoFactorAuth\\app\\Providers\\TwoFactorAuthServiceProvider"
      ],
      "aliases": {
        "TwoFactorAuth": "MHMartinez\\TwoFactorAuth\\Facade"
      }
    }
  }
}
